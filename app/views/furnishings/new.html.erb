<h1>New furnishing</h1>

<% form_for(@furnishing) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :img %><br />
    <%= f.text_field :img %>
  </p>
  <p>
    <%= f.label :dimensions_description %><br />
    <%= f.text_area :dimensions_description %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', furnishings_path %>
